{% block _order_review_payment_methods_novalnet_credit_card_widget -%}
    {% if options.payment_method is defined %}
        <div class="hidden"
             data-page-component-module="novalnet/js/app/components/novalnet-component"
             data-page-component-options="{{ {paymentMethod: options.payment_method}|json_encode }}">
        </div>
    {% endif %}
{%- endblock %}
